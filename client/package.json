{
  "name": "4h-military-partnership",
  "version": "0.4.0",
  "private": false,
  "proxy": "http://localhost:3333/",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "axios": "^0.18.0",
    "downshift": "^3.1.5",
    "match-sorter": "^2.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0-alpha.0",
    "react-cache": "^2.0.0-alpha.1",
    "react-dom": "^16.7.0-alpha.0",
    "react-scripts": "^2.1.1",
    "react-spring": "^5.9.2",
    "styled-components": "^4.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "deploy": "now && now alias",
    "now-build": "npm run coverage && npm run build",
    "style": "stylelint"
  },
  "devDependencies": {
    "@types/faker": "^4.1.4",
    "@types/jest": "^23.3.8",
    "@types/match-sorter": "^2.2.0",
    "@types/node": "^10.12.3",
    "@types/reach__router": "^1.2.2",
    "@types/react": "^16.7.1",
    "@types/react-dom": "^16.0.9",
    "@types/styled-components": "^4.0.3",
    "faker": "^4.1.0",
    "jest-watch-typeahead": "^0.2.0",
    "react-testing-library": "^5.2.3",
    "stylelint": "^9.7.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6"
  },
  "jest": {
    "coverageReporters": [
      "json",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "src/**/*.ts",
      "src/**/*.tsx",
      "!/**/*.d.ts",
      "!src/registerServiceWorker.ts",
      "!src/index.tsx",
      "!src/utils/mixins.ts",
      "!src/utils/api.ts",
      "!src/types.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 50,
        "functions": 80,
        "lines": 80
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
